{
	"name": "expo-signed",
	"version": "1.0.0",
	"description": "Effortlessly configure signing for Android builds in Expo projects during the prebuild process.",
	"author": "Akay Akagunduz",
	"license": "MIT",
	"keywords": [
		"expo",
		"plugin",
		"android",
		"signing",
		"keystore",
		"prebuild"
	],
	"publishConfig": {
		"access": "public"
	},
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/akayakagunduz/expo-signed.git"
	},
	"engines": {
		"node": ">=16"
	},
	"dependencies": {
		"@expo/config-plugins": "~9.0.17"
	},
	"expo": {
		"plugins": [
			"expo-signed"
		]
	}
}
